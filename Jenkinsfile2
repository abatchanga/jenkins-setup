pipeline{
    agent any
    tools {
        maven 'M2_HOME'
        }
    stages{
        stage('sonarqube scan'){
            steps{
                withSonarQubeEnv('sonarserver'){
                sh 'mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -Dsonar.projectKey=abatchanga_week16-code-geo2'
                //sh 'mvn sonar:sonar'
            }
            }
        }
        stage('all the maven commands'){
            steps{
                sh 'mvn clean test compile install package'
            }
        }
    }
}